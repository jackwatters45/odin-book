{
	"name": "odin-book",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "DEBUG=app* vite",
		"build": "tsc && vite build",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
		"format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss,md,html,json,yml}\" --write",
		"compile": "tsc",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,js,cjs}": [
			"npm run lint:fix",
			"npm run format"
		],
		"*.{md,html,css,json,yml}": "prettier --write"
	},
	"dependencies": {
		"@fingerprintjs/fingerprintjs": "^4.0.1",
		"@jackwatters/simple-nav": "^1.3.2",
		"@mdi/js": "^7.2.96",
		"@mdi/react": "^1.6.1",
		"@tanstack/react-query": "^4.29.15",
		"@tanstack/react-query-devtools": "^4.29.15",
		"libphonenumber-js": "^1.10.41",
		"lodash": "^4.17.21",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-modal-pure": "^1.0.6",
		"react-router": "^6.14.2",
		"react-router-dom": "^6.14.2"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.0.2",
		"@tanstack/eslint-plugin-query": "^4.29.9",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/debug": "^4.1.8",
		"@types/lodash": "^4.14.199",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@vitejs/plugin-react-swc": "^3.0.0",
		"@vitest/coverage-v8": "^0.34.1",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-node": "^0.3.7",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.3.5",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"husky": "^8.0.0",
		"jest": "^29.5.0",
		"jest-styled-components": "^7.1.1",
		"jsdom": "^22.1.0",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.7",
		"react-hook-form": "^7.45.4",
		"styled-components": "^6.0.7",
		"typescript": "^5.0.2",
		"vite": "^4.3.0",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.34.1"
	}
}
